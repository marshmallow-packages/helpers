{
    "name": "marshmallow/helpers",
    "description": "A package containing all the helper function we can use throughout all our projects",
    "keywords": ["marshmallow"],
    "homepage": "https://github.com/Marshmallow-Development/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "stef@marshmallow.dev"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/helpers": "^1.3",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.2"
    },
    "autoload": {
        "psr-4": {"Marshmallow\\HelperFunctions\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Marshmallow\\HelperFunctions\\Tests\\": "tests"}
    },
     "scripts": {
        "test": "vendor/bin/phpunit --coverage-text"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\HelperFunctions\\ServiceProvider"
            ],
            "aliases": {
                "Builder": "Marshmallow\\HelperFunctions\\Facades\\Builder",
                "Date": "Marshmallow\\HelperFunctions\\Facades\\Date",
                "Ip": "Marshmallow\\HelperFunctions\\Facades\\Ip",
                "Review": "Marshmallow\\HelperFunctions\\Facades\\Review",
                "Str": "Marshmallow\\HelperFunctions\\Facades\\Str",
                "URL": "Marshmallow\\HelperFunctions\\Facades\\URL",
                "User": "Marshmallow\\HelperFunctions\\Facades\\User"
            }
        }
    }
}
