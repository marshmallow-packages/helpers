{
    "name": "marshmallow/breadcrumb",
    "description": "A package for Laravel Breadcrum usage with the ability to add some default and use your custom templating",
    "keywords": ["marshmallow"],
    "homepage": "https://github.com/Marshmallow-Development/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "stef@marshmallow.dev"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "marshmallow/commands": "^1.0"
    },
    "autoload": {
        "psr-4": {"Marshmallow\\HelperFunctions\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Marshmallow\\HelperFunctions\\": "src"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\HelperFunctions\\HelperFunctionsServiceProvider"
            ],
            "aliases": {
                "Str": "Marshmallow\\HelperFunctions\\Facades\\Str"
            }
        }
    }
}